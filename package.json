{
  "name": "stream-lines",
  "description": "Line transform stream, reads bytes writes arrays of lines.",
  "version": "1.1.5",
  "author": "muji <noop@xpm.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tmpfs/stream-lines.git"
  },
  "main": "lib/line-stream.js",
  "dependencies": {
    "through3": "~1.1.1"
  },
  "devDependencies": {
    "chai": "~3.4.1",
    "coveralls": "~2.11.6",
    "istanbul": "~0.4.1",
    "mocha": "~2.3.4"
  },
  "engine": [
    "node >= 0.10.11"
  ],
  "keywords": [
    "stream",
    "lines",
    "parse",
    "read"
  ],
  "scripts": {
    "docs": "npm run readme",
    "readme": "mdp --force -v",
    "test": "NODE_ENV=test mocha -- test/spec",
    "cover": "NODE_ENV=test istanbul cover --root ./lib _mocha -- test/spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "mdp": {
    "title": "Stream Lines",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/tmpfs/stream-lines",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
